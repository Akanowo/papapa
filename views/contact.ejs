<script>
	// Initialize and add the map
	function initMap() {
		// The location of Papapa
		const papapa = { lat: 6.6049273, lng: 3.2429381 };
		// The map, centered at Papapa
		const map = new google.maps.Map(document.getElementById("map"), {
			zoom: 4,
			center: papapa,
		});
		// The marker, positioned at Uluru
		const marker = new google.maps.Marker({
			position: papapa,
			map: map,
		});
	}
</script>
